{
	"name": "pozitronik/yii2-dbmon",
	"description": "Database monitoring for Yii2",
	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"database",
		"sql",
		"module"
	],
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.0",
		"pozitronik/yii2-traits": "^1.0.0",
		"kartik-v/yii2-grid": "dev-master"
	},
	"require-dev": {
		"yiisoft/yii2": "dev-master",
		"yiisoft/yii2-bootstrap4": "~2.0.10"
	},
	"autoload": {
		"psr-4": {
			"pozitronik\\dbmon\\": "src"
		}
	},
	"config": {
		"fxp-asset": {
			"enabled": false
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
